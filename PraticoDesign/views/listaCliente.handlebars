<div class="cardHeader">
    <h2>Lista de Clientes</h2> 
        <button href="#" class="btn" id="open" onclick="location.href='cadastroCLiente';">
            <span><i class="far fa-user"></i></span>
            Incluir Cliente
        </button>  
</div>
<table class="list" id="clientList">
	<tr>
		<th>Nome</th>
		<th>Data de Nascimento</th>
		<th>E-mail</th>
		<th>IE/RG</th>
		<th>CPF/CNPJ</th>
		<th>Telefone</th>
		<th>Rua</th>
		<th>Bairro</th>
		<th>Cidade</th>
		<th>Estado</th>
		<th>CEP</th>
	</tr>                       
    
	{{#data}}
    <tr>
		<td>{{nome}}</td>
		<td>{{data_nascimento}}</td>
		<td>{{email}}</td>
		<td>{{ie_rg}}</td>
		<td>{{cpf_cnpj}}</td>
		<td>{{telefone}}</td>
		<td>{{rua}}</td>
		<td>{{bairro}}</td>
		<td>{{cidade}}</td>
		<td>{{estado}}</td>
		<td>{{cep}}</td>
	</tr>                        
	{{/data}}
</table>

